package com.pack;


import java.util.Scanner;

public class EvenToNnumber {

	public static void main(String[] args) {
		Scanner scan = new Scanner("System.in");
		System.out.println("Enter the n Number: ");
		int num = scan.nextInt();
		
		for(int i = 1; i<num ; i++) {
			
			if (i%2==0) {
				System.out.print(i);
			}
		}
		scan.close();
	}

}

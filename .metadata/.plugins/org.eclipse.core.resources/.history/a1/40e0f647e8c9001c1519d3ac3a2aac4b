		int last = args.length-1;
		
		while(first < last) {
			if(args.)
		}
	}

	public static void main(String[] args) {
		
		System.out.println("Lenght of String : "+args[0].length());
		System.out.print("UpperCase : "+args[0].toUpperCase());
	}

